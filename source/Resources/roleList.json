    {
      "displayname": "Button Barry",
      "modifier": "ButtonBarry",
      "color": "ButtonBarry",
      "appliesto": "All",
      "description": "Button Barry has the ability to call a meeting from anywhere on the map, even during sabotages. They have the same amount of meetings as a regular player."
    },
    {
      "displayname": "Flash",
      "modifier": "Flash",
      "color": "Flash",
      "appliesto": "All",
      "description": "The Flash travels at a faster speed in comparison to a normal player."
    },
    {
      "displayname": "Giant",
      "modifier": "Giant",
      "color": "Giant",
      "appliesto": "All",
      "description": "The Giant is a gigantic Crewmate, that has a decreased walk speed."
    },
    {
      "displayname": "Lovers",
      "modifier": "Lovers",
      "color": "Lovers",
      "appliesto": "All",
      "description": "The Lovers are two players who are linked together.These two players get picked randomly between Crewmates and Impostors.n/They gain the primary objective to stay alive together.n/If they are both among the last 3 players, they win.n/In order to do so, they gain access to a private chat, only visible by them in between meetings.n/However, they can also win with their respective team, hence why the Lovers do not know the role of the other lover."
    },
    {
      "displayname": "Radar",
      "modifier": "Radar",
      "color": "Radar",
      "appliesto": "All",
      "description": "The Radar is a crewmate who knows where the closest player is to them."
    },
    {
      "displayname": "Sleuth",
      "modifier": "Sleuth",
      "color": "Sleuth",
      "appliesto": "All",
      "description": "The Sleuth is a crewmate who gains knowledge from reporting dead bodies.n/During meetings the Sleuth can see the roles of all players in which they've reported."
    },
    {
      "displayname": "Tiebreaker",
      "modifier": "Tiebreaker",
      "color": "Tiebreaker",
      "appliesto": "All",
      "description": "If any vote is a draw, the Tiebreaker's vote will go through.n/If they voted another player, they will get voted out."
    },

    {
      "displayname": "Disperser",
      "modifier": "Disperser",
      "color": "Impostor",
      "appliesto": "Impostors",
      "description": "The Disperser is an Impostor who has a 1 time use ability to send all players to a random vent.n/This includes miner vents. However, Disperser does not appear on Airship or Submerged."
    },
    {
      "displayname": "Double Shot",
      "modifier": "DoubleShot",
      "color": "Impostor",
      "appliesto": "Impostors",
      "description": "Double Shot is an Impostor who gets an extra life when assassinating.n/Once they use their life they are indicated with a red flashn/and can no longer guess the person who they guessed wrong for the remainder of that meeting."
    },
    {
      "displayname": "Underdog",
      "modifier": "Underdog",
      "color": "Impostor",
      "appliesto": "Impostors",
      "description": "The Underdog is an Impostor with a prolonged kill cooldown.n/When they are the only remaining Impostor, they will have their kill cooldown shortened."
    }
